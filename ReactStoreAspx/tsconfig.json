{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "moduleResolution": "node",
    "noResolve": false,
    "noImplicitAny": false,
    "removeComments": true,
    "sourceMap": true,
    "allowJs": false,
    "jsx": "react"
  },
  "include": [ "./Content/src/**/*" ],
  "exclude": [
    "./plugins/**/*",
    "./typings/**/*",
    "./built/**/*",
    "node_modules",
    "wwwroot",
    "Scripts/*"

  ]
}